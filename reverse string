import java.lang.*;
import java.io.*;
import java.util.*;
public class reverse1 {

    public static void main(String[] args)

    {

    String str = "Nandhini";  
    String str1="";

    //Get the String length

    //int iStrLength=stringInput.length();    

    //Using For loop

for(int i=str.length()-1;i>=0;-- i)

{
str1=str1+str.charAt(i);
}
System.out.println("  " +str1); 

}

}
